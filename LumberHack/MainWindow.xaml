<Window x:Class="LumberHack.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LumberHack"
        mc:Ignorable="d"
        Title="MainWindow" ResizeMode="NoResize" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="472,371,0,0" TextWrapping="Wrap" Text="CPS:" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="508,369,0,0" TextWrapping="Wrap" Text="Clicks Per Second" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <Button Content="Down" HorizontalAlignment="Left" Margin="382,369,0,0" VerticalAlignment="Top" Width="75" Click="Button_Right"/>
        <Button Content="Up" HorizontalAlignment="Left" Margin="291,369,0,0" VerticalAlignment="Top" Width="75" Click="Button_Left"/>
        <Button Content="Start Game" HorizontalAlignment="Left" Margin="337,335,0,0" VerticalAlignment="Top" Width="75" Click="Button_Space"/>

    </Grid>
</Window>
